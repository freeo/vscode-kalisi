{
    "name": "Kalisi",
    "type": "light",
    "semanticHighlighting": true,
    "tokenColors": [
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#70a0d0"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "string.quoted.docstring",
            ],
            "settings": {
                "foreground": "#0060a0",
                "fontStyle": ""
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#0070c0"
            }
        },
        {
            "name": "Number Float",
            "scope": [
                "constant.numeric.floating-point",
                "constant.numeric.float",
            ],
            "settings": {
                "foreground": "#00a0a0"
            }
        },
        {
            "name": "Built-in constant",
            "scope": [
                "constant.language",
                "meta.use.rust", // [true,false]
            ],
            "settings": {
                "foreground": "#3c91e6"
            }
        },
        {
            "name": "hmm",
            "scope": [
                "constant.character",
            ],
            "settings": {
                "foreground": "#66b600",
                "fontStyle": "bold"
            }
        },
        {
            "name": "hmm",
            "scope": [
                "constant.other",
                "string.quoted.rune",
            ],
            "settings": {
                "foreground": "#00a0a0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character.format.placeholder",
                "constant.other.placeholder",
            ],
            "settings": {
                "foreground": "#a200ff",
                "fontStyle": ""
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable.language",
                "variable.parameter.function"
            ],
            "settings": {
                "foreground": "#202090",
                "fontStyle": ""
            }
        },

        {
            // Typescript Types
            "name": "Types",
            "scope": [
                "support.type",
            ],
            "settings": {
                "foreground": "#ae01e2",
                "fontStyle": ""
            }
        },

        {
            // JSON Key
            "name": "Types",
            "scope": [
                "support.type.property-name.json",
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "keyword.other.fn.rust",
                "keyword.other.crate.rust"
            ],
            "settings": {
                "foreground": "#66b600"
            }
        },
        {
            "name": "Keyword Other Rust",
            "scope": [
                "keyword",
                "keyword.other.rust"
            ],
            "settings": {
                "foreground": "#8400ff"
            }
        },
        {
            "name": "Keyword Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#000000",
                // "fontStyle": "bold"
            }
        },
        {
            "name": "Keyword Operators",
            "scope": "keyword.operator.borrow",
            "settings": {
                "foreground": "#ae01e2"
            }
        },
        {
            "name": "Keyword Control flow",
            "scope": [
                "keyword.control.flow",
                "keyword.control"
            ],
            "settings": {
                "foreground": "#1177dd",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Keyword Control import",
            "scope": [
                "keyword.control.import",
                "keyword.import",
                "keyword.package"
            ],
            "settings": {
                "foreground": "#d80050",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#66b600"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "",
                "foreground": "#ae01e2"
            }
        },
        {
            "name": "Storage type function",
            "scope": [
                "storage.type.function",
                "keyword.function",
                "storage.type.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#66b600"
            }
        },
        {
            "name": "Storage type (let)",
            "scope": [
                "storage.type.ts",
                "storage.type.rust"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#229000"
            }
        },
        {
            "name": "Class name",
            "scope": "entity.name.class",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#ffbf69"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other",
            "settings": {
                "fontStyle": "",
                "foreground": "#008b00"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#3c91e6",
                "fontStyle": "bold",
            }
        },
        {
            "name": "Function Call",
            "scope": "meta.function-call",
            "settings": {
                // "foreground": "#3c91e6"
                "foreground": "#000000"
            }
        },
        {
            "name": "Function Call Arguments",
            "scope": "meta.function-call.arguments",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                // "foreground": "#3c91e6"
                "foreground": "#000000"
            }
        },
        {
            // Typescript inline parameters
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "",
                // "foreground": "#9495aa"
                "foreground": "#229000"
            }
        },
        {
            // HTML tags
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                // "foreground": "#fa824c"
                "foreground": "#1177dd"
            }
        },
        {
            // HTML attribue
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "bold",
                // "foreground": "#ffbf69"
                "foreground": "#000"
            }
        },
        {
            // CSS constant
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "bold",
                // "foreground": "#ffbf69"
                "foreground": "#0070c0"
            }
        },
        {
            // Typescript class
            "name": "Library class/type",
            "scope": [
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                // "foreground": "#ffbf69"
                "foreground": "#ae01e2"
            }
        },
        {
            // Typescript console.
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#66b600"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                // "background": "#664e4d",
                "foreground": "#e45e59",
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                // "background": "#664e4d",
                "foreground": "#cf433e"
            }
        },
        {
            "name": "JSON String",
            "scope": "string.quoted.double.json",
            "settings": {
                "foreground": "#1daf00"
            }
        },
        {
            "name": "diff.header",
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#aaaaaa"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#ff5f00"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#A6E22E"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#5f5fd7"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "#8FBE00A0"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "scope": "keyword.other",
            "settings": {
                "foreground": "#848484"
            }
        },
        {
            "scope": [
                "meta.property-value",
                "support.constant.property-value",
                "constant.other.color"
            ],
            "settings": {
                // "foreground": "#9fd356"
                "foreground": "#0070c0"
            }
        },
        {
            "name": "Function argument",
            "scope": "meta.property-name support.type.property-name",
            "settings": {
                // "fontStyle": "normal"
                "fontStyle": ""
            }
        },
        {
            "name": "Function argument",
            "scope": "meta.property-value punctuation.separator.key-value",
            "settings": {
                "foreground": "#444444"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "keyword.other.use",
                "keyword.other.function.use",
                "keyword.other.namespace",
                "keyword.other.new",
                "keyword.other.special-method",
                "keyword.other.unit",
                "keyword.other.use-as"
            ],
            "settings": {
                "foreground": "#fa824c"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "meta.use support.class.builtin",
                "meta.other.inherited-class support.class.builtin"
            ],
            "settings": {
                "foreground": "#fa824c",
                "fontStyle": ""
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.other",
            "settings": {
                "foreground": "#000000",
                "fontStyle": ""
            }
        },
        {
            "name": "Golang variables",
            "scope": [
                "variable.other.assignment",
                "variable.other.declaration",
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": ""
            }
        },
        {
            "name": "Coffeescript Function argument",
            "scope": "variable.parameter.function.coffee",
            "settings": {
                "foreground": "#9fd356",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown Titles",
            "scope": [
                "entity.name.section.markdown",
                "punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#66b600",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Raw",
            "scope": "markup.raw.inline.markdown",
            "settings": {
                "foreground": "#9fd356"
            }
        },
        {
            "name": "Markdown bold stars",
            "scope": [
                "punctuation.definition.bold.markdown",
                "punctuation.definition.italic.markdown"
            ],
            "settings": {
                "foreground": "#fa824c"
            }
        },
        {
            "name": "Markdown link title braces",
            "scope": [
                "punctuation.definition.string.begin.markdown",
                "punctuation.definition.string.end.markdown"
            ],
            "settings": {
                "foreground": "#fa824c"
            }
        },
        {
            "name": "Markdown link braces",
            "scope": "punctuation.definition.metadata.markdown",
            "settings": {
                "foreground": "#fa824c"
            }
        },
        {
            "name": "Markdown link",
            "scope": [
                "markup.underline.link.markdown",
                "markup.underline.link.image.markdown",
                "meta.image.inline.markdown"
            ],
            "settings": {
                "foreground": "#fa824c",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold/italic",
            "scope": [
                "markup.bold.markdown",
                "markup.italic.markdown"
            ],
            "settings": {
                "foreground": "#fa824c"
            }
        },
        {
            "name": "Markdown bold/italic",
            "scope": "markup.italic.markdown",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown bold/italic",
            "scope": "markup.bold.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown pre",
            "scope": "markup.raw.block.markdown",
            "settings": {
                "foreground": "#664e4d"
            }
        },
        {
            "name": "GitGutter deleted",
            "scope": "markup.deleted.git_gutter",
            "settings": {
                "foreground": "#ff4f00"
            }
        },
        {
            "name": "GitGutter inserted",
            "scope": "markup.inserted.git_gutter",
            "settings": {
                "foreground": "#a7da1e"
            }
        },
        {
            "name": "GitGutter changed",
            "scope": "markup.changed.git_gutter",
            "settings": {
                "foreground": "#5f5fd7"
            }
        },
        {
            // Typescript inline `${}`
            "name": "Template expressions.",
            "scope": "meta.template.expression",
            "settings": {
                // "foreground": "#444444"
                "foreground": "#cc2200",
            }
        },
        {
            "name": "Keyword Issue.",
            "scope": "keyword.issue",
            "settings": {
                "foreground": "#ff6600",
                // "background": "#ff6600"
            }
        },
        {
            "scope": "keyword.todo",
            "name": "Keyword Todo.",
            "settings": {
                "foreground": "#ffaa00",
                // "background": "#ffaa00"
            }
        },
        {
            "name": "Keyword Done.",
            "scope": "keyword.done",
            "settings": {
                "foreground": "#a7da1e",
                // "background": "#a7da1e"
            }
        }
    ],

